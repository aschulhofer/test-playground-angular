{
  "name": "angular-playground",
  "private": true,
  "version": "0.0.0",
  "description": "A starter project for AngularJS - Playground",
  "repository": "",
  "devDependencies": {
    "bower": "^1.7.7",
    "glob": "^7.0.5",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.9.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.1",
    "protractor": "^3.2.2",
    "watch": "^0.19.1"
  },
  "scripts": {
    "lint": "jshint .",
    "clean": "rimraf app/dist",
    "create:dist": "node scripts/mkdist.js && echo '' > app/dist/bundle.js && echo '' > app/dist/bundle.css",
    "preconcat": "npm run clean && npm run create:dist",
    "concat": "node scripts/glob-concat.js",
    "watch:js": "watch \"npm run concat\" app --wait=2 --ignoreDirectoryPattern=\"/(.*)(bower_components|dist)(.*)/g\"",
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "update-index-async": "node -e \"var fs=require('fs'),indexFile='app/index-async.html',loaderFile='app/bower_components/angular-loader/angular-loader.min.js',loaderText=fs.readFileSync(loaderFile,'utf-8').split(/sourceMappingURL=angular-loader.min.js.map/).join('sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map'),indexText=fs.readFileSync(indexFile,'utf-8').split(/\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/).join('//@@NG_LOADER_START@@\\n'+loaderText+'    //@@NG_LOADER_END@@');fs.writeFileSync(indexFile,indexText);\""
  },
  "dependencies": {
    "angular-jwt": "0.0.9"
  }
}
